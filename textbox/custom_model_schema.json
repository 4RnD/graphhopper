{
  "$id": "gh-custom-model",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GraphHopper Custom Model",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "speed": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "if": {
            "type": "string"
          },
          "else if": {
            "type": "string"
          },
          "else": {
            "type": "null"
          },
          "multiply by": {
            "type": "number"
          },
          "limit to": {
            "type": "number"
          }
        }
      }
    },
    "priority": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "if": {
            "type": "string"
          },
          "else if": {
            "type": "string"
          },
          "else": {
            "type": "null"
          },
          "multiply by": {
            "type": "number"
          },
          "limit to": {
            "type": "number"
          }
        }
      }
    },
    "distance_influence": {
      "type": "number"
    },
    "areas": {
      "type": "object"
    }
  }
}